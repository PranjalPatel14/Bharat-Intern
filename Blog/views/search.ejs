<div class="author">
    <h1 class="authir_heading">Search Results</h1>
</div>

<section class="articles">
    <ul class="article-ul">
        <% data.forEach(post=> { %>
            <ul class="article-ul">
                <li>
                    <a href="/post/<%= post._id %>">
                        <span>
                            <%= post.title %>
                        </span>
                        <span class="article-list__date">
                            <%= post.createdAt.toDateString() %>
                        </span>
                    </a>
                </li>
            </ul>
    
        <% }) %>
    </ul>
</section>